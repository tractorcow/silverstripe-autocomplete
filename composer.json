{
	"name": "tractorcow/silverstripe-autocomplete",
	"description": "Autocomplete text field for Silverstripe",
	"type": "silverstripe-vendormodule",
	"keywords": [
		"silverstripe",
		"autocomplete"
	],
	"authors": [
		{
			"name": "Damian Mooyman",
			"email": "damian.mooyman@gmail.com",
			"homepage": "http://github.com/tractorcow",
			"role": "Developer"
		}
	],
	"support": {
		"issues": "http://github.com/tractorcow/silverstripe-autocomplete/issues"
	},
	"require": {
		"silverstripe/framework": "^6"
	},
    "require-dev": {
        "phpunit/phpunit": "^11.5"
    },
	"extra": {
        "expose": [
            "css",
            "javascript"
        ]
	},
    "autoload": {
        "psr-4": {
            "TractorCow\\AutoComplete\\": "src/",
            "TractorCow\\AutoComplete\\Tests\\": "tests/php/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
